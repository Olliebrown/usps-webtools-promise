    "@types/node": "^16.11.64",
    "ava": "^5.1.1",
    "c8": "^7.12.0",
    "eslint": "^8.33.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16"
  }
